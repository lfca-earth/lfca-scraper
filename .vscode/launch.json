{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "lfca-scraper",
      "args": ["src/index.ts"],
      "console": "integratedTerminal",
      "cwd": "${workspaceRoot}",
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register/transpile-only"],
      "request": "launch",
      "runtimeExecutable": "node",
      "skipFiles": ["<node_internals>/**", "node_modules/**"],
      "type": "node"
    }
  ]
}
